{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm exec tailwindcss -i ./styles/tailwind.src.css -o ./styles/tailwind.css --watch\" \"next dev\" \"pnpm ts:lint\"",
    "build": "concurrently \"pnpm exec tailwindcss -i ./styles/tailwind.src.css -o ./styles/tailwind.css --minify\" \"next build\"",
    "start": "next start",
    "lint": "next lint",
    "ts:lint": "tsc --noEmit --incremental --watch"
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "concurrently": "^7.6.0",
    "eslint": "8.31.0",
    "next": "13.1.1",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-unused-imports": "^2.0.0"
  }
}
